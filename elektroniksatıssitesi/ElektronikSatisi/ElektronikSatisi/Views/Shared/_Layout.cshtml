                                                <li>@Html.ActionLink("Huawei", "Telefon", new { id = "HUAWEI" })</li>
                                                <li>@Html.ActionLink("Blackberry", "Telefon", new { id = "BLACBERRY" })</li>
                                                <li>@Html.ActionLink("General Mobile", "Telefon", new { id = "GENERAL MOBILE" })</li>
